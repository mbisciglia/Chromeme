{
    "manifest_version": 2,
    "name": "Chromeme Extension",
    "version": "0.1",
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["content.js"]
        }
      ],
      "browser_action": {
        "default_icon": "pepe.png",
        "default_popup": "popup.html"
        
      }
    }